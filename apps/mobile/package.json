{
  "name": "@tempo-tune/mobile",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prestart": "bash scripts/generate-dev-config.sh",
    "start": "react-native start",
    "android": "bash scripts/generate-dev-config.sh && react-native run-android",
    "ios": "bash scripts/generate-dev-config.sh && react-native run-ios",
    "ios:device": "bash scripts/generate-dev-config.sh && react-native run-ios --device",
    "ios:device:udid": "bash scripts/generate-dev-config.sh && react-native run-ios --device --udid",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "version:update": "node scripts/update-app-version.js",
    "version:inject": "node scripts/inject-version.js"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-native": "0.81.5",
    "react-native-webview": "^13.12.3"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.2",
    "@babel/runtime": "^7.25.2",
    "@react-native-community/cli": "^20.1.1",
    "@react-native-community/cli-platform-ios": "^20.1.1",
    "@react-native/babel-preset": "^0.81.5",
    "@react-native/eslint-config": "^0.81.5",
    "@react-native/metro-config": "^0.81.5",
    "@react-native/typescript-config": "^0.81.5",
    "@types/react": "^19.1.0",
    "@types/react-test-renderer": "^19.0.0",
    "babel-jest": "^29.7.0",
    "eslint": "^9.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "react-test-renderer": "^19.1.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": "^24.0.0"
  }
}
